					/>
				</tr>
				<tr id="row2" class="row colPadding">
					<TableData
						firstRow={false}
						title="Max. Lifting Height (mm)"
						values={["60", "60", "60", "60", "60"]}
					/>
				</tr>
				<tr class="row colPadding">
					<TableData
						firstRow={false}
						title="Max. Payload Weight (kg)"
						values={[
							"800",
							"1200-1500",
							"3000/5000",
							"5000/10000",
							"10000/50000",
						]}
					/>
				</tr>

				<tr class="row colPadding">
					<TableData
						firstRow={false}
						title="Pallet Size (mm)"
						values={[
							"L1200*W1000",
							"L1200*W1000",
							"Customized",
							"Customized",
							"Customized",
						]}
					/>
				</tr>
				<tr class="row colPadding">
					<TableData
						firstRow={false}
						title="Navigation"
						values={["SLAM", "SLAM", "SLAM", "SLAM", "SLAM"]}
					/>
				</tr>
				<tr class="row colPadding">
					<TableData
						firstRow={false}
						title="Driving Type"
						values={[
							"Tow Steering Wheel Differential Driving",
							"Tow Steering Wheel Differential Driving",
							"Omnibearing Steering Wheel Differential Driving",
							"Omnibearing Steering Wheel Differential Driving",
							"Omnibearing Steering Wheel Differential Driving",
						]}
					/>
				</tr>
				<tr class="row colPadding">
					<TableData
						firstRow={false}
						title="Motor Type"
						values={[
							"--",
							"--",
							"Servo Motor",
							"Servo Motor",
							"Servo Motor",
						]}
					/>
				</tr>
				<tr class="row colPadding">
					<TableData
						firstRow={false}
						title="Obstacle Avoidance"
						values={[
							"Safety Lidar",
							"Safety Lidar",
							"Safety Lidar",
							"Safety Lidar",
							"Safety Lidar",
						]}
					/>
				</tr>
				<tr class="row colPadding">
					<TableData
						firstRow={false}
						title="L/W/H (mm)"
						values={[
							"980/680/245",
							"1160/860/245",
							"Customized",
							"Customized",
							"Customized",
						]}
					/>
				</tr>
			</tbody>
		</table>
	</main>
</Layout>
